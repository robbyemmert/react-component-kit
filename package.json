  "devDependencies": {
    "@types/jest": "^22.2.2",
    "autoprefixer": "^8.2.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
